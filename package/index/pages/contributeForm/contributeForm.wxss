}

.uploading_text {
  font-size: 28rpx;
  line-height: 76rpx;
  color: #aaa;
}

.uploading_parent {
  display: flex;
  flex-wrap: wrap;
}

.uploading_first {
  width: 18.4%;
  height: 135rpx;
  margin-right: 2%;
  margin-bottom: 20rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}

.uploading_first:nth-child(5n) {
  margin-right: 0%;
}

.uploading {
  font-size: 30rpx;
  line-height: 60rpx;
  color: #ccc;
}

.mmxx {
  width: 135rpx;
