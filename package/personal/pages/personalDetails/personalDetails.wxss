.main-wrapper {
  padding-top: 20rpx;
  height: 100%;
  box-sizing: border-box;
}

.oneself_message {
  background: #fff;
}

.oneself_news {
  display: flex;
  width: 92%;
  padding: 0rpx 4%;
  border-bottom: 2rpx solid #f5f5f5;
}

.oneself_identity {
  width: 28%;
}

.oneself_identity text {
  font-size: 32rpx;
  line-height: 152rpx;
  color: #333;
}

.oneself_kb {
  width: 50%;
}

.oneself_P {
  width: 16%;
  height: 110rpx;
  margin-top: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}

.person_avatar {
  width: 100%;
  height: 100%;
}

.arrows {
  width: 4%;
  padding-left: 2%;
}

.arrows image {
  width: 12rpx;
  height: 30rpx;
  margin-top: 54rpx;
}

.oneself_news2 {
  display: flex;
  width: 92%;
  padding: 0 4%;
  border-bottom: 2rpx solid #f5f5f5;
}

.oneself_identity2 {
  width: 40%;
}

.oneself_identity2 text {
  font-size: 32rpx;
  line-height: 90rpx;
  color: #333;
}

.oneself_kb2 {
  width: 32%;
}

.oneself_P2 {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  width: 72%;
  overflow: hidden;
}

.arrows2 {
  width: 4%;
  padding-left: 2%;
}

.arrows2 image {
  width: 12rpx;
  height: 30rpx;
  margin-top: 34rpx;
}
