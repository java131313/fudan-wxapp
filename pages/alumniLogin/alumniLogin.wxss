.login-wrapper {
  width: 100%;
  height: 100%;
  padding: 87rpx 9.2%;
  box-sizing: border-box;
}

.logoPic {
  height: 120rpx;
  width: 100%;
}

.logoPic image {
  height: 100%;
  width: 82.64%;
}

.submissionInformation {
  margin-top: 31rpx;
}

.submissionInformation .loginBtn {
  color: #cc1a1a;
  font-size: 32rpx;
  line-height: 83rpx;
  height: 83rpx;
  margin-bottom: 75rpx;
  background: #ffe5e5;
}

picker {
  background: #fff;
  line-height: 94rpx;
  height: 94rpx;
  margin-top: 21rpx;
  border-radius: 6rpx;
  margin-bottom: 53rpx;
  position: relative;
}

.picker-text{
  margin-left: 20rpx;
  color: #aaa;
  font-size: 30rpx;
}

.input_time_pic {
  position: absolute;
  top: 23%;
  right: 5%;
  height: 45rpx;
  width: 45rpx;
  z-index: 10;
}